// Write a c code that asks the user to enter the two integers and prints their sum
#include <stdio.h>

int main() {
    int int1,int2,int3;
    printf("enter the int1\n");
    scanf("%d",&int1);
    printf("enter the int2\n");
    scanf("%d",&int2);
    int3=(int1+int2);
    printf("the sum of int1 and int2 is %d\n",int3);

    return 0;
}
