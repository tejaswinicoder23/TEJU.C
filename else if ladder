#include <stdio.h>

int main() {
    int marks;
    printf("enter your marks\n");
    scanf("%d",&marks);
    if(marks==90)
    {
        printf("gradeA\n");
    }
    else if(marks>=80)
    {
        printf("gradeB\n");
    }
    else if(marks>=70)
    {
        printf("gradeC\n");
    }
    else if(marks<=30)
    {
        printf("gradeD\n");
    }
    }
    
