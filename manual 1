#include<stdio.h>
#include<math.h>
int main()
{
    float x1,x2,y1,y2,distance;
    printf("Enter the coordinate point 1\n");;
    scanf("%f%f",&x1,&y1);
      printf("Enter the coordinate point 2\n");;
    scanf("%f%f",&x2,&y2);
    distance=sqrt((x2-x1)*(x2-x1)+(y2-y1)*(y2-y1));
    printf("the straight distance between two points is %f\n",distance);
    
}
