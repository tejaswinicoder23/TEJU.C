#include<stdio.h>
    int passedstudents(int m1,int m2,int m3){
        if(m1>=40 && m2>=40 && m3&&40)
        return 1;
        else 
        return 0;
        
    }

int main(){
    int sub1,sub2,sub3,avg;
    printf("Enter the marks for subject 1,subject 2,subject 3\n");
    scanf("%d%d%d",&sub1,&sub2,&sub3);
    avg=(sub1+sub2+sub3)/100;
    printf("average marks %d\n",avg);
    if(passedstudents(sub1,sub2,sub3))
        printf("PASS\n");
        else
        printf("FAIL\n");
    
}

    
