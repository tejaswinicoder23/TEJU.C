#include <stdio.h>

int main() {
   int row,col;
   printf("Enter the rows and coloumns of an array\n");
   scanf("%d%d",&row,&col);
   int a[row][col],b[row][row],sum[row][col];
   
   for(int i=0;i<row;i++){
       for(int j=0;j<col;j++);
        printf("Enter the elements of A\n");
       scanf("%d",&a[i][j]);
   }
   
   for(int i=0;i<row;i++){
       for(int j=0;j<col;j++);
        printf("Enter the elements of B\n");
       scanf("%d",&b[i][j]);
   }
    for(int i=0;i<row;i++){
       for(int j=0;j<col;j++);
       sum[i][j]=a[i][j]+b[i][j];
    }
    printf("%d\n",sum[i][j]);
   }
   
