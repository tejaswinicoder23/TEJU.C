#include <stdio.h>

int main() {
int num,ori,rem,rev=0;
printf("enter the number:");
scanf("%d",&num);
ori=num;
while(num!=0){
    rem=num%10;
    rev=rev*10+rem;
    num=num/10;
    
}
if(ori==rev){
    printf("the number is palindrome ");
}
    else{
    printf("the number is not a palindrome ");
    
}
}

