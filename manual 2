#include <stdio.h>
int main() {
    int marks;
    printf("enter the marks of the students\n");
    scanf("%d",&marks);
    if(marks>=90){
        printf("Grade A \n");
    }else if(marks>=75){
        printf("Grade B");
    }else if(marks>=60){
        printf("Grade C");
    }else if(marks>=50){
        printf("Grade D");  
    }else {
        printf("fail");
    }
